<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--bold-->
    <font
        android:font="@font/pretendard_bold"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/pretendard_bold"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <font
        android:font="@font/agro_bold"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/agro_bold"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <!--medium-->
    <font
        android:font="@font/pretendard_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/pretendard_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/agro_medium"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/agro_medium"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <!--regular-->
    <font
        android:font="@font/pretendard_regular"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/pretendard_regular"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <font
        android:font="@font/agro_regular"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/agro_regular"
        app:fontStyle="normal"
        app:fontWeight="1000" />
</font-family>